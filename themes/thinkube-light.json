  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#787878",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#2f855a"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#c9a040"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#805ad5"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.other"
      ],
      "settings": {
        "foreground": "#c9a040"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#005570",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#0088aa"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage"
      ],
      "settings": {
        "foreground": "#005570"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#c99820",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Inherited class",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#d69e2e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#0088aa"
      }
    },
    {
      "name": "Function argument",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#555555",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag name",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#005570",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#0088aa"
      }
    },
    {
      "name": "Library function",
      "scope": [
        "support.function",
        "support.macro"
      ],
      "settings": {
        "foreground": "#0088aa"
      }
    },
    {
      "name": "Library constant",
      "scope": [
        "support.constant"
      ],
      "settings": {
        "foreground": "#805ad5"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#c99820"
      }
    },
    {
      "name": "Library variable",
      "scope": [
        "support.variable"
      ],
      "settings": {
        "foreground": "#0088aa"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid"
      ],
      "settings": {
        "foreground": "#ffffff",
        "background": "#c53030"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#ffffff",
        "background": "#805ad5"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#005570"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#005570",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#333333",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#0088aa"
      }
    },
    {
      "name": "Markdown - Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#2f855a"
      }
    }
  ]
}
