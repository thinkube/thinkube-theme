  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#64748b",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#34d399"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#FF6B35"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#c084fc"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.other"
      ],
      "settings": {
        "foreground": "#FF6B35"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#e2e8f0"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#22d3ee",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#0088aa"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage"
      ],
      "settings": {
        "foreground": "#22d3ee"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#fbbf24",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Inherited class",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#fcd34d",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#38bdf8"
      }
    },
    {
      "name": "Function argument",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#cbd5e0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag name",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#22d3ee",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#0088aa"
      }
    },
    {
      "name": "Library function",
      "scope": [
        "support.function",
        "support.macro"
      ],
      "settings": {
        "foreground": "#38bdf8"
      }
    },
    {
      "name": "Library constant",
      "scope": [
        "support.constant"
      ],
      "settings": {
        "foreground": "#c084fc"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#fbbf24"
      }
    },
    {
      "name": "Library variable",
      "scope": [
        "support.variable"
      ],
      "settings": {
        "foreground": "#0088aa"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid"
      ],
      "settings": {
        "foreground": "#ffffff",
        "background": "#ef4444"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#ffffff",
        "background": "#c084fc"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#0088aa"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#22d3ee",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#e2e8f0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#0088aa"
      }
    },
    {
      "name": "Markdown - Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#34d399"
      }
    }
  ]
}
